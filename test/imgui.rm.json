{
   "$rel" : [
      {
         "#/additional_rm_loaded/imgui.rm.dll" : {
            "$obj" : {
               "PathFolder" : ".\\",
               "FileName" : "imgui.rm.dll"
            },
            "$rel" : "#/RVM/load/dll",
            "$sub" : "#"
         }
      },
      {
         "$obj" : [ "#/Calc", "#/DebugCalc" ],
         "$rel" : "#/imgui/viewport",
         "$sub" : {
            "title" : "imgui.rm.dll main_frame"
         }
      }
   ],
   "Calc" : {
      "$obj" : [ "./ReadOut", "./btnClear", "./btnClearEntry", "./btnSeven" ],
      "$rel" : "#/imgui/form",
      "$sub" : {
         "visible" : true,
         "title" : "Calculator"
      },
      "ReadOut" : {
         "$obj" : "./ReadOut/value",
         "$rel" : "#/imgui/text",
         "$sub" : {
            "visible" : true
         },
         "value" : "0"
      },
      "Accum" : 0.0,
      "FlagNewNum" : false,
      "PendingOp" : "",
      "Clear" : [
         {
            "#/Calc/Accum" : {
               "/" : 0.0
            },
            "#/Calc/PendingOp" : {
               "/" : ""
            }
         },
         "#/Calc/ClearEntry"
      ],
      "btnClear" : {
         "$obj" : "#/Calc/Clear",
         "$rel" : "#/imgui/button",
         "$sub" : {
            "text" : "C",
            "width" : "50",
            "height" : "40"
         }
      },
      "ClearEntry" : [
         {
            "#/Calc/ReadOut/value" : {
               "/" : "0"
            },
            "#/Calc/FlagNewNum" : {
               "/" : true
            }
         }
      ],
      "btnClearEntry" : {
         "$obj" : "#/Calc/ClearEntry",
         "$rel" : "#/imgui/button",
         "$sub" : {
            "text" : "CE",
            "width" : "50",
            "height" : "40"
         }
      },
      "NumPressed" : [
         {
            "$obj" : "#/Calc/FlagNewNum",
            "$rel" : "#/then",
            "$sub" : {
               "#/Calc/ReadOut/value" : {
                  "$obj" : "$obj",
                  "$rel" : "#/string/="
               },
               "#/Calc/FlagNewNum" : {
                  "/" : false
               }
            }
         },
         {
            "$obj" : "#/Calc/FlagNewNum",
            "$rel" : "#/else",
            "$sub" : [
               {
                  "$obj" : "#/Calc/ReadOut/value",
                  "$rel" : {
                     "/is_value_zero" : "#/=="
                  },
                  "$sub" : "#/Calc/zero_str"
               },
               {
                  "$obj" : "/is_value_zero",
                  "$rel" : "#/then",
                  "$sub" : {
                     "#/Calc/ReadOut/value" : {
                        "$obj" : "$obj",
                        "$rel" : "#/string/="
                     }
                  }
               },
               {
                  "$obj" : "/is_value_zero",
                  "$rel" : "#/else",
                  "$sub" : {
                     "#/Calc/ReadOut/value" : {
                        "$obj" : "$obj",
                        "$rel" : "#/string/+="
                     }
                  }
               }
            ]
         }
      ],
      "btnSeven" : {
         "$obj" : {
            "$obj" : "./$sub/text",
            "$rel" : "#/Calc/NumPressed"
         },
         "$rel" : "#/imgui/button",
         "$sub" : {
            "text" : "7",
            "width" : "50",
            "height" : "40"
         }
      },
      "zero_str" : "0"
   },
   "DebugCalc" : {
      "$obj" : [
         {
            "/data" : {
               "$obj" : "#/Calc",
               "$rel" : "#/json/dump"
            }
         },
         {
            "$obj" : "/data",
            "$rel" : "#/imgui/TextUnformatted",
            "$sub" : {
               "visible" : true
            }
         },
         "#/json/object"
      ],
      "$rel" : "#/imgui/form",
      "$sub" : {
         "visible" : true,
         "title" : "Calc Debug"
      }
   }
}
