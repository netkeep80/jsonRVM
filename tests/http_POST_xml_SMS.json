                  "$sub" : "$rel/Modem/request/header/__RequestVerificationToken"
               },
               {
                  "$obj" : "$rel/Modem/request",
                  "$rel" : "http/POST/xml"
               }
            ],
            "default" : null
         }
      }
   ],
