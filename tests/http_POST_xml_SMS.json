                           }
                        },
                        {
                           "$obj" : {
                              "$obj" : "$ent/Content",
                              "$rel" : "="
                           },
                           "$rel" : "tag",
                           "$sub" : {
                              "<>" : "Content"
                           }
                        },
                        {
                           "$obj" : {
                              "$obj" : "$ent/Length",
                              "$rel" : "="
                           },
                           "$rel" : "tag",
                           "$sub" : {
                              "<>" : "Length"
                           }
                        },
                        {
                           "$obj" : {
                              "$obj" : "$ent/Reserved",
                              "$rel" : "="
                           },
                           "$rel" : "tag",
                           "$sub" : {
                              "<>" : "Reserved"
                           }
                        },
                        {
                           "$obj" : {
                              "$obj" : "$ent/Date",
                              "$rel" : "="
                           },
                           "$rel" : "tag",
                           "$sub" : {
                              "<>" : "Date"
                           }
                        }
                     ],
                     "$rel" : "xml",
                     "$sub" : {
                        "<>" : "request"
                     }
                  }
               },
               {
                  "$obj" : "$rel/Modem/SesTokInfo/body/response/token",
