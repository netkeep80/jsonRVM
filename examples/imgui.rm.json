            "$rel" : "#/Calc/NumPressed"
         },
         "$rel" : "#/imgui/button",
         "$sub" : {
            "text" : "0",
            "width" : "108",
            "height" : "40"
         }
      },
      "Decimal" : {
         "init_val" : "0.",
         "dot" : ".",
         "minus_one" : -1,
         "$rel" : {
            "$obj" : "#/Calc/FlagNewNum",
            "$rel" : "#/switch/bool",
            "$sub" : {
               "true" : {
                  "#/Calc/ReadOut/value" : {
                     "$obj" : "./init_val",
                     "$rel" : "#/string/="
                  },
                  "#/Calc/FlagNewNum" : {
                     "$obj" : "#/false",
                     "$rel" : "#/="
                  }
               },
               "false" : [
                  {
                     "$obj" : "#/Calc/ReadOut/value",
                     "$rel" : {
                        "/index_of_dot" : "#/string/find"
                     },
                     "$sub" : "./dot"
                  },
                  {
                     "$obj" : "/index_of_dot",
                     "$rel" : {
                        "/is_value_minus_one" : "#/=="
                     },
                     "$sub" : "./minus_one"
                  },
                  {
                     "$obj" : "/is_value_minus_one",
                     "$rel" : "#/then",
                     "$sub" : {
                        "#/Calc/ReadOut/value" : {
                           "$obj" : "./dot",
                           "$rel" : "#/string/+="
                        }
                     }
                  }
               ]
            }
         }
      },
      "btnDecimal" : {
         "$obj" : {
            "$obj" : "$sub/text",
            "$rel" : "#/Calc/Decimal"
         },
         "$rel" : "#/imgui/button",
         "$sub" : {
            "text" : ".",
            "width" : "50",
            "height" : "40"
         }
      },
      "zero_str" : "0",
      "AccumInitVal" : 0.0,
      "PendingOpInitVal" : "",
      "ReadOutInitValue" : "0"
   },
   "DebugCalc" : {
      "$obj" : [
         "#/json/object",
         {
            "/data" : {
               "$obj" : "#",
               "$rel" : "#/json/dump"
            }
         },
         {
            "$obj" : "/data",
            "$rel" : "#/imgui/TextUnformatted",
            "$sub" : {
               "visible" : true
            }
         },
         "#/json/object"
      ],
      "$rel" : "#/imgui/form",
      "$sub" : {
         "visible" : true,
         "title" : "Calc Debug"
      }
   },
   "true" : true,
   "false" : false
}
